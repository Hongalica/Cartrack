<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".activity.NameList" android:id="@+id/fragment_namelist">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/header_text" android:textSize="30sp"
                android:text="@string/master_namelist" android:textAlignment="center" android:textColor="#000000"/>
        <ListView
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:scrollbars="vertical" android:id="@+id/master_name_list"
                android:headerDividersEnabled="true"  android:textColor="#000000"/>
    </LinearLayout>
</FrameLayout>